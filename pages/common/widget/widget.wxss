.iconText {
  width:120rpx;
  text-align: center;
  margin:40rpx 25rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}


.iconText-icon {
  width:80rpx;
  height:80rpx;
}


.iconText-text{
  color:#888;
  font-size: 30rpx;
}

.iconText-list{
  height: 210rpx;
  display: flex;
  flex-direction: column;
  flex:1;
  overflow: hidden;
  text-align: left;
  margin:10rpx ;
  font-size: 26rpx;
}

.iconText-name{
  font-size: 36rpx;
  color:#000;
}

